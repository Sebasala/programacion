---
import { getCollection } from "astro:content";
import * as content from "@content/pages/marketing-digital/content.md";
import categories from "@pages/categories";
import CategoryPage from "@components/CategoryPage.astro";

const devPosts = (await getCollection("marketing")).sort(
	(a, b) => a.data.pubDate.valueOf() - b.data.pubDate.valueOf(),
);
---

<CategoryPage
	category={categories["marketing-digital"]}
	content={content}
	devPosts={devPosts}
/>
